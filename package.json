{
  "name": "react-example",
  "productName": "Nyrox",
  "description": "Nyrox Application",
  "author": "User",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron-main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server.ts",
    "electron": "cross-env NODE_ENV=development electron .",
    "electron:build": "npm run build && cross-env NODE_ENV=production electron .",
    "build": "cross-env NODE_ENV=production vite build",
    "preview": "vite preview",
    "clean": "rimraf dist",
    "lint": "tsc --noEmit",
    "dist": "npm run build && electron-builder",
    "generate-assets": "capacitor-assets generate --ios"
  },
  "build": {
    "appId": "com.nyrox.app",
    "productName": "Nyrox",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron-main.js",
      "server.ts",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "resources/icon.png"
    }
  },
  "dependencies": {
    "@capacitor/cli": "^8.1.0",
    "@capacitor/core": "^8.1.0",
    "@capacitor/ios": "^8.1.0",
    "@google/genai": "^1.29.0",
    "@tailwindcss/vite": "^4.1.14",
    "@types/cors": "^2.8.19",
    "@vitejs/plugin-react": "^5.0.4",
    "better-sqlite3": "^12.4.1",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "fs-extra": "^11.3.3",
    "lucide-react": "^0.546.0",
    "motion": "^12.23.24",
    "nodemailer": "^8.0.1",
    "openai": "^6.22.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "vite": "^6.2.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@types/express": "^4.17.21",
    "@types/node": "^22.14.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.1.0",
    "electron": "^40.6.0",
    "electron-builder": "^26.8.1",
    "rimraf": "^6.1.3",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
